{
  "name": "i18n-hover",
  "publisher": "hss",
  "displayName": "i18n-hover",
  "description": "show specific translation when you hover on a tag",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact",
    "workspaceContains:.vscode/i18n-hover.json",
    "onCommand:i18n-hover.jumpToTranslation"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "i18n-hover.jumpToTranslation",
        "title": "i18n Hover: Jump to Translation Key (from Hover)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/8tme/i18n-hover"
  },
  "keywords": [
    "i18n",
    "translation",
    "hover"
  ]
}
